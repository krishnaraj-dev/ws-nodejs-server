{
  "extends": "./server.json",
  "compilerOptions": {
    "outDir": "dist",
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "**/*.js"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
